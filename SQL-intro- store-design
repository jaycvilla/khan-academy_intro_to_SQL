CREATE TABLE store_supply (id INTEGER PRIMARY KEY, name TEXT, quantity INTEGER, price_per_unit INTEGER);

INSERT INTO store_supply VALUES(1,"bike", 30, 300);
INSERT INTO store_supply VALUES(2,"helmet", 45, 25);
INSERT INTO store_supply VALUES(3,"brakes", 300, 50);
INSERT INTO store_supply VALUES(4,"wheels", 60, 100);
INSERT INTO store_supply VALUES(5,"pegs", 100, 20);

SELECT * FROM store_supply GROUP BY price_per_unit;
SELECT SUM(quantity) FROM store_supply;
